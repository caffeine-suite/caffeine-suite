{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "dependencies": {
    "art-build-configurator": "^1.26.14",
    "art-standard-lib": "^1.69.4",
    "caffeine-eight": "^2.6.0"
  },
  "description": "Caffeine.SourceMap",
  "devDependencies": {
    "art-testbench": "^2.2.1",
    "jest": "^26.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/Jest/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "caffeine-source-map",
  "scripts": {
    "build": "caf -p -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "2.0.0",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
