{
  "author": "Shane Brinkman-Davis Delamore",
  "bugs": "https:/github.com/caffeine-suite/caffeine-suite/issues",
  "dependencies": {
    "art-standard-lib": "^1.70.0",
    "caffeine-eight": "^2.6.5",
    "neptune-namespaces-runtime": "^3.2.6"
  },
  "description": "Caffeine.SourceMap",
  "devDependencies": {
    "art-build-configurator": "^1.28.0",
    "art-testbench": "^2.2.8",
    "jest": "^26.6.1"
  },
  "homepage": "https://github.com/caffeine-suite/caffeine-suite",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/Jest/**/*.js",
      "!source/test/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "caffeine-source-map",
  "repository": {
    "type": "git",
    "url": "https://github.com/caffeine-suite/caffeine-suite.git"
  },
  "scripts": {
    "build": "caf -m -c source-source -o source; nn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "jest",
    "watch": "npm-watch"
  },
  "version": "2.0.6",
  "watch": {
    "build": {
      "extensions": "caf",
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
