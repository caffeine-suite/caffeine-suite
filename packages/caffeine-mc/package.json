    "cardinal": "^1.0.0",
    "chalk": "^1.1.3",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "fs-extra": "^3.0.0",
    "glob-promise": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "description": "Select, configure and extend your to-JavaScript compiler, with arbitrary code, on a per file bases from within the file.",
  "devDependencies": {
    "art-testbench": "^2.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chai": "^4.2.0",
    "coffee-loader": "^0.7.3",
    "css-loader": "^3.6.0",
    "json-loader": "^0.5.7",
    "mocha": "^6.2.3",
    "mock-fs": "^4.12.0",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "three": "^0.109.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-stylish": "^0.1.8"
  },
  "homepage": "https://github.com/caffeine-suite/caffeine-suite",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "caffeine-mc",
  "repository": {
    "type": "git",
    "url": "https://github.com/caffeine-suite/caffeine-suite.git"
  },
  "scripts": {
    "build": "coffee --output source --compile source-source &\ncaf -p -c source-source -o source &\nwait\nnn -s",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "test": "nn -s;mocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "3.0.5",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
