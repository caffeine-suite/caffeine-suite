{
  "author": "Shane Brinkman-Davis Delamore, Imikimi LLC",
  "bugs": "https:/github.com/caffeine-suite/caffeine-script/issues",
  "config": {
    "blanket": {
      "pattern": "source"
    }
  },
  "dependencies": {
    "art-binary": "^1.1.7",
    "art-class-system": "1.11.6",
    "art-object-tree-factory": "^2.2.2",
    "art-standard-lib": "^1.69.4",
    "caffeine-eight": "^2.6.0",
    "caffeine-mc": "^3.0.6",
    "caffeine-script-runtime": "^1.13.3",
    "caffeine-source-map": "^2.0.0",
    "source-map": "^0.7.2"
  },
  "description": "CaffeineScript makes programming more wonderful, code more beautiful and programmers more productive. It is a lean, high-level language that empowers you to get the most out of any JavaScript runtime.",
  "devDependencies": {
    "art-build-configurator": "^1.26.14",
    "art-testbench": "^2.2.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.3",
    "mock-fs": "^4.12.0"
  },
  "homepage": "https://github.com/caffeine-suite/caffeine-script",
  "jest": {
    "collectCoverageFrom": [
      "source/**/*.js",
      "!source/_?[Jjt]est/**/*.js"
    ],
    "testEnvironment": "node"
  },
  "license": "ISC",
  "name": "caffeine-script",
  "repository": {
    "type": "git",
    "url": "git@github.com:shanebdavis/caffeine-script.git"
  },
  "scripts": {
    "build": "caf -v -p -c cafInCaf -o source",
    "build-clean": "npm run clean; npm run build",
    "clean": "rm -rf source/*",
    "perf": "nn -s;mocha -u tdd perf",
    "test": "nn -s;mocha -u tdd",
    "watch": "npm-watch"
  },
  "version": "0.72.3",
  "watch": {
    "build": {
      "extensions": [
        "caf",
        "coffee"
      ],
      "patterns": [
        "source-source"
      ],
      "quiet": false
    }
  }
}
